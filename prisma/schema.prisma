generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Products {
    UUID        String @id(map: "products_uuid") @unique() @default(uuid()) @db.Char(36)
    Name        String @db.VarChar(20)
    Description String @db.VarChar(500)
    Price       Float
    Quantity    Int
    CreatedAt   DateTime
    UpdatedAt   DateTime
 }